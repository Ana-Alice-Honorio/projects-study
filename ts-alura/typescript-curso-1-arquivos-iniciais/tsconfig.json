{
    "compilerOptions": {
        "outDir": "dist/js",
        // pasta que vou ler
        "target": "ES6",
        // em qual versão do js vou compilar
        "noEmitOnError": true,
        //  não vai gerar arquivo js
        "noImplicitAny": true
    },
    "include": [
        // quais docs serão compilados
        "app/**/*"
    ]
}